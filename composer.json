{
    "name": "buckhamduffy/coding-standards",
    "description": "",
    "type": "library",
    "require": {
        "php": ">=8.3.0",
        "driftingly/rector-laravel": "^2.0.5",
        "larastan/larastan": "^3.4.0",
        "laravel/framework": "^12.16.0",
        "phpstan/phpstan": "^2.1.17",
        "rector/rector": "^2.0.16",
        "symplify/easy-coding-standard": "^12.5.18",
        "captainhook/captainhook": "^5.25.2",
        "symfony/process": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5.21",
        "spatie/laravel-data": "^4.15.1",
        "captainhook/hook-installer": "^1.0.4",
        "spatie/laravel-ray": "^1.40.2"
    },
    "autoload": {
        "psr-4": {
            "BuckhamDuffy\\CodingStandards\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Aaron Florey",
            "email": "aaron@buckhamduffy.com"
        },
        {
            "name": "Alex Buckham",
            "email": "alex@buckhamduffy.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "captainhook/hook-installer": true
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Tests\\Unit\\": "tests/Unit/",
            "Tests\\Unit\\Rector\\": "tests/Unit/Rector/"
        }
    }
}
