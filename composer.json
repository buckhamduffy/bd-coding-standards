{
    "name": "buckhamduffy/coding-standards",
    "description": "",
    "type": "library",
    "require": {
        "php": ">=8.2.1",
        "driftingly/rector-laravel": "2.0.5",
        "larastan/larastan": "2.9.8",
        "laravel/framework": "^11.14.0",
        "phpstan/phpstan": "^1.11.6",
        "rector/rector": "^1.2.0",
        "symplify/easy-coding-standard": "12.3.5",
        "captainhook/captainhook": "^5.23",
        "symfony/process": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.2.6",
        "spatie/laravel-data": "^4.7.1",
        "captainhook/hook-installer": "^1.0",
        "spatie/laravel-ray": "^1.37"
    },
    "autoload": {
        "psr-4": {
            "BuckhamDuffy\\CodingStandards\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Aaron Florey",
            "email": "aaron@buckhamduffy.com"
        },
        {
            "name": "Alex Buckham",
            "email": "alex@buckhamduffy.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "platform": {
            "php": "8.2.1"
        },
        "allow-plugins": {
            "captainhook/hook-installer": true
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Tests\\Unit\\": "tests/Unit/",
            "Tests\\Unit\\Rector\\": "tests/Unit/Rector/"
        }
    }
}
